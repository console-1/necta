{
  "name": "n8n Agent Chat Interface Design System",
  "version": "2.0.0",
  "description": "Design system extracted from ui_9.html focused on chat interface, LangSmith analytics, and Airtop session management MVP",
  "mvp_focus": [
    "chat_interface",
    "langsmith_analytics", 
    "airtop_session_management"
  ],

  "colors": {
    "primary": {
      "brand": "#dd6b5c",
      "accent": "#4CAF50",
      "success": "#4CAF50",
      "warning": "#FF9800",
      "error": "#F44336"
    },
    "neutral": {
      "background": "#1A1A1A",
      "surface": "#2D2D2D",
      "surface-elevated": "#3A3A3A",
      "surface-hover": "#404040",
      "border": "#404040",
      "border-subtle": "#2A2A2A",
      "text-primary": "#FFFFFF",
      "text-secondary": "#B0B0B0",
      "text-muted": "#808080"
    },
    "workflow": {
      "node-default": "#4A4A4A",
      "connection": "#666666"
    }
  },

  "typography": {
    "fontFamily": {
      "primary": ["Inter", "-apple-system", "BlinkMacSystemFont", "Segoe UI", "sans-serif"]
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem", 
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem"
    },
    "fontWeight": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeight": {
      "tight": 1.25,
      "normal": 1.5,
      "relaxed": 1.6
    }
  },

  "spacing": {
    "xs": "0.25rem",
    "sm": "0.5rem",
    "md": "0.75rem",
    "lg": "1rem",
    "xl": "1.5rem",
    "2xl": "2rem"
  },

  "borderRadius": {
    "sm": "0.25rem",
    "md": "0.375rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "full": "50%"
  },

  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.3)",
    "md": "0 4px 6px -1px rgba(0, 0, 0, 0.4)",
    "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.5)"
  },

  "layout": {
    "dashboard": {
      "grid": "280px 1fr 350px",
      "rows": "60px 1fr",
      "areas": [
        "sidebar header analytics",
        "sidebar main analytics"
      ]
    },
    "sidebar": {
      "width": "280px"
    },
    "analytics_panel": {
      "width": "350px"
    },
    "header": {
      "height": "60px"
    }
  },

  "components": {
    "chat": {
      "messages": {
        "background": "radial-gradient(circle, #404040 1px, transparent 1px)",
        "backgroundSize": "20px 20px",
        "padding": "1rem 1.5rem",
        "gap": "1rem"
      },
      "message": {
        "avatar": {
          "size": "32px",
          "borderRadius": "50%",
          "background": "#3A3A3A"
        },
        "content": {
          "background": "#2D2D2D",
          "border": "1px solid #404040",
          "borderRadius": "0.75rem",
          "padding": "0.75rem 1rem",
          "maxWidth": "70%"
        },
        "user": {
          "avatar": {
            "background": "#FF5722"
          },
          "content": {
            "background": "#FF5722",
            "borderColor": "#FF5722"
          }
        }
      },
      "input": {
        "background": "#2D2D2D",
        "border": "1px solid #404040",
        "borderRadius": "0.75rem",
        "padding": "0.75rem 1rem",
        "minHeight": "2.5rem",
        "maxHeight": "8rem"
      }
    },

    "analytics": {
      "card": {
        "background": "#2D2D2D",
        "border": "1px solid #404040",
        "borderRadius": "0.5rem",
        "padding": "1rem"
      },
      "metric": {
        "title": {
          "fontSize": "0.75rem",
          "color": "#B0B0B0",
          "textTransform": "uppercase",
          "letterSpacing": "0.05em"
        },
        "value": {
          "fontSize": "1.5rem",
          "fontWeight": 700,
          "color": "#FFFFFF"
        }
      },
      "chart": {
        "height": "200px",
        "background": "transparent"
      }
    },

    "session_management": {
      "browser_preview": {
        "background": "#2D2D2D",
        "border": "1px solid #404040",
        "borderRadius": "0.5rem",
        "height": "200px"
      },
      "session_controls": {
        "background": "#2D2D2D",
        "border": "1px solid #404040",
        "borderRadius": "0.5rem",
        "padding": "1rem"
      },
      "status_indicator": {
        "active": {
          "background": "#4CAF50",
          "animation": "pulse 2s infinite"
        },
        "inactive": {
          "background": "#808080"
        },
        "error": {
          "background": "#F44336"
        }
      }
    },

    "button": {
      "primary": {
        "background": "#FF5722",
        "color": "#FFFFFF",
        "border": "none",
        "borderRadius": "0.375rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": 500,
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      },
      "secondary": {
        "background": "#2D2D2D",
        "color": "#FFFFFF",
        "border": "1px solid #404040",
        "borderRadius": "0.375rem",
        "padding": "0.5rem 1rem",
        "fontSize": "0.875rem",
        "fontWeight": 500,
        "cursor": "pointer",
        "transition": "all 0.2s ease"
      }
    },

    "navigation": {
      "sidebar": {
        "background": "#1A1A1A",
        "borderRight": "1px solid #2A2A2A",
        "padding": "1rem"
      },
      "nav_item": {
        "padding": "0.75rem 1rem",
        "borderRadius": "0.375rem",
        "fontSize": "0.875rem",
        "color": "#B0B0B0",
        "transition": "all 0.2s ease",
        "cursor": "pointer"
      },
      "nav_item_active": {
        "background": "#2D2D2D",
        "color": "#FFFFFF",
        "borderLeft": "3px solid #FF5722"
      }
    },

    "header": {
      "background": "#1A1A1A",
      "borderBottom": "1px solid #2A2A2A",
      "padding": "0 1.5rem",
      "height": "60px"
    },

    "environment_selector": {
      "background": "#2D2D2D",
      "border": "1px solid #404040",
      "borderRadius": "0.375rem",
      "padding": "0.5rem 1rem",
      "color": "#FFFFFF",
      "fontSize": "0.875rem",
      "cursor": "pointer",
      "transition": "all 0.2s ease"
    }
  },

  "animations": {
    "pulse": {
      "keyframes": {
        "0%": { "opacity": 1 },
        "50%": { "opacity": 0.5 },
        "100%": { "opacity": 1 }
      },
      "duration": "2s",
      "iteration": "infinite"
    },
    "hover_lift": {
      "transform": "translateY(-1px)",
      "opacity": 0.8
    },
    "transitions": {
      "default": "all 0.2s ease"
    }
  },

  "patterns": {
    "chat_canvas_grid": {
      "backgroundImage": "radial-gradient(circle, #404040 1px, transparent 1px)",
      "backgroundSize": "20px 20px",
      "backgroundPosition": "0 0"
    }
  },

  "breakpoints": {
    "sm": "640px",
    "md": "768px", 
    "lg": "1024px",
    "xl": "1280px"
  },

  "zIndex": {
    "modal": 1000,
    "dropdown": 900,
    "header": 100,
    "sidebar": 50
  }
}